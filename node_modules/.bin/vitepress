#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_less@4.2.0_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/bin/node_modules:/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_less@4.2.0_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/node_modules:/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_less@4.2.0_postcss@8.4.38_search-insights@2.13.0/node_modules:/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_less@4.2.0_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/bin/node_modules:/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_less@4.2.0_postcss@8.4.38_search-insights@2.13.0/node_modules/vitepress/node_modules:/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/vitepress@1.1.4_@algolia+client-search@4.23.3_less@4.2.0_postcss@8.4.38_search-insights@2.13.0/node_modules:/home/stevesuk/omega/BakaXL-Next-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
